#!perl

use 5.010001;
use strict;
use warnings;
use Perinci::CmdLine 1.04;

# DATE
# VERSION

our %SPEC;

# add -V as the alias for --detail, for convenience
require App::ListSoftwareLicenses;
{
    no warnings;
    $App::ListSoftwareLicenses::SPEC{list_software_licenses}
        {args}{detail}{cmdline_aliases} = {V=>{}};
}

Perinci::CmdLine->new(
    log_any_app => 0,
    url => '/App/ListSoftwareLicenses/list_software_licenses',
)->run;

1;
# ABSTRACT: List all Software::License's
# PODNAME: list-software-licenses

=head1 SYNOPSIS

 % list-software-licenses
 % list-software-licenses --help

=cut
